<%= render "layouts/errors", :object => @product %>

<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <%= f.label :origin %>
  <%= f.text_field :origin %>
  <%= f.label :cost %>
  <%= f.number_field :cost %>
  <%= f.submit  %>
<% end %>
